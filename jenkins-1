pipeline {
    agent any
    stages {
        stage('Debug SCP') {
            steps {
                script {
                    // Debugging the environment and commands
                    echo "Checking if SCP is available"
                    sh 'which scp'  // or 'where scp' on Windows
                    sh 'scp -V'    // Check SCP version
                    sh 'ls -l'     // Ensure Dockerfile exists in the workspace
                }
            }
        }
    }
}
